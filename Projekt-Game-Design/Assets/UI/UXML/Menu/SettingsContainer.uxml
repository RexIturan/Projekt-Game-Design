<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="/Assets/UI/Styles/Button_Layout.uss" />
    <ui:VisualElement style="justify-content: center; align-items: center; width: 100%; height: 100%;">
        <ui:VisualElement name="settingsContainer" style="height: 80%; width: 35%; justify-content: space-around; align-items: center; background-color: rgba(0, 0, 0, 0.53);">
            <ui:Label text="Settings" display-tooltip-when-elided="true" style="font-size: 50px; -unity-font-style: bold; color: rgb(255, 255, 255); position: relative; top: auto;" />
            <ui:VisualElement name="SoundSettingsContainer">
                <ui:Label text="Sound" display-tooltip-when-elided="true" style="align-items: stretch; justify-content: flex-start; font-size: 25px; color: rgb(255, 255, 255); -unity-font-style: bold; -unity-text-align: upper-center;" />
                <ui:Label text="Ambient:" display-tooltip-when-elided="true" style="align-items: stretch; justify-content: flex-start; font-size: 18px; color: rgb(255, 255, 255); -unity-font-style: bold;" />
                <ui:Slider picking-mode="Ignore" value="42" high-value="100" usage-hints="None" show-input-field="false" inverted="false" direction="Horizontal" name="AmbientSlider" style="width: 236px;" />
                <ui:Label text="Music:" display-tooltip-when-elided="true" style="align-items: stretch; justify-content: flex-start; font-size: 18px; color: rgb(255, 255, 255); -unity-font-style: bold;" />
                <ui:Slider picking-mode="Ignore" value="42" high-value="100" usage-hints="None" show-input-field="false" inverted="false" direction="Horizontal" name="MusicSlider" style="width: 236px;" />
            </ui:VisualElement>
            <ui:VisualElement name="GraphicSettingsContainer">
                <ui:Label text="Graphic" display-tooltip-when-elided="true" style="align-items: stretch; justify-content: flex-start; font-size: 25px; color: rgb(255, 255, 255); -unity-font-style: bold; -unity-text-align: upper-center;" />
                <ui:Label text="Quality:" display-tooltip-when-elided="true" style="align-items: stretch; justify-content: flex-start; font-size: 18px; color: rgb(255, 255, 255); -unity-font-style: bold;" />
                <ui:Slider picking-mode="Ignore" value="42" high-value="100" usage-hints="None" show-input-field="false" inverted="false" direction="Horizontal" name="QualitySlider" style="width: 236px;" />
                <ui:Label text="Resolution:" display-tooltip-when-elided="true" style="align-items: stretch; justify-content: flex-start; font-size: 18px; color: rgb(255, 255, 255); -unity-font-style: bold;" />
                <ui:Slider picking-mode="Ignore" value="42" high-value="100" usage-hints="None" show-input-field="false" inverted="false" direction="Horizontal" name="ResulotionSlider" style="width: 236px;" />
            </ui:VisualElement>
            <ui:Button text="Back" display-tooltip-when-elided="true" name="BackButton" class="Button_Layout" />
        </ui:VisualElement>
        <ui:VisualElement name="Content" picking-mode="Ignore" style="width: 100%; height: 10%; position: absolute; bottom: 0;" />
    </ui:VisualElement>
</ui:UXML>
